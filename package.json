{
  "name": "dncl-vscode",
  "displayName": "DNCL Language Support (共通テスト手順記述標準言語)",
  "description": "Adds support for DNCL (共通テスト手順記述標準言語) Language",
  "publisher": "natureofmad",
  "version": "0.0.2",
  "preview": true,
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/natureofmad/dncl-vscode"
  },
  "keywords": [
    "DNCL",
    "共通テスト",
    "大学入試センター"
  ],
  "contributes": {
    "languages": [{
      "id": "dncl",
      "aliases": ["DNCL", "dncl"],
      "extensions": [".dnc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dncl",
      "scopeName": "source.dncl",
      "path": "./syntaxes/dncl.tmLanguage.json"
    }],
    "snippets": [{
        "language": "dncl",
        "scopeName": "source.dnc",
        "path": "./snippets/dncl.code-snippets"
    }]
  }
}
